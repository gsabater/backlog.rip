{
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "consistent",
  "tabWidth": 2,
  "printWidth": 100,

  "arrowParens": "always",
  "disableLanguages": [
    "json",
    "jsonc"
  ],

  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false
}
